{
    "files.eol": "\n",
    "editor.tabSize": 2,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "eslint.format.enable": true,
    "eslint.validate": [
        "typescript",
        "typescriptreact"
    ],
    "[typescript][typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "jest.autoRun": null
}